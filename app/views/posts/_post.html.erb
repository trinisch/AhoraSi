<div id="<%= dom_id post %>">

  <p>
    <strong>Título:</strong>
    <%= post.titulo %>
  </p>

  <p>
    <strong>Sigla del ramo:</strong>
    <%= post.sigla_ramo %>
  </p>

  <p>
    <strong>Precio:</strong>
    <%= post.precio %>
  </p>

  <p>
    <strong>Descripción:</strong>
    <%= post.descripcion %>
  </p>

  <p>
    <strong>Profesor:</strong>
    <%= post.user.name %>
  </p>


</div>
